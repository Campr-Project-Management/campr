{% trans_default_domain 'messages' %}

{% extends 'MainBundle::base.html.twig' %}

{% block body_content %}
    {% if app.user is object %}
    <div class="container-fluid">
        <section class="section first-section">
            <div class="row">
                {% if app.user.teams.count %}
                <div class="col-xl-6">
                    <div class="team-list-wrapper">
                        <h3 class="page-header">{{ 'message.your_teams'|trans }}</h3>
                        <ul class="thumb-list">
                            {% for team in app.user.teams %}
                            <li>
                                <div class="row justify-content-between align-items-center">
                                    <div class="col-md-auto">
                                        {% if team.logo %}
                                            {% set teamStyle = 'background-image: url("' ~ vich_uploader_asset(team, 'logoFile') ~ '")' %}
                                        {% else %}
                                            {% set teamStyle = 'background: #123456;' %}
                                        {% endif %}
                                        <a href="{{ path('main_team_sso', {id: team.id}) }}" class="team-img" style="{{ teamStyle }}"></a>
                                        <div class="team-info">
                                            <div class="team-name">{{ team }}</div>
                                            <div class="team-details">
                                                {% if team.teamMembers.count > "1" %}
                                                    {{ team.teamMembers.count }} {{ 'message.members'|trans }}
                                                {% else %}
                                                    {{ team.teamMembers.count }} {{ 'message.member'|trans }}
                                                {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-auto text-right">
                                        <a href="{{ path('main_team_sso', {id: team.id}) }}" class="button button-small">
                                            {{ 'button.login'|trans }}
                                        </a>
                                    </div>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                {% endif %}

                {% if app.user.teamMembers.count %}
                <div class="col-xl-6">
                    <div class="team-list-wrapper">
                        <h3 class="page-header">{{ 'message.teams'|trans }}</h3>
                        <ul class="thumb-list">
                            {% for teamMember in app.user.teamMembers %}
                            <li>
                                <div class="row justify-content-between align-items-center">
                                    <div class="col-md-auto">
                                        {% if teamMember.team and teamMember.team.logo %}
                                            {% set teamStyle = 'background-image: url("' ~ vich_uploader_asset(teamMember.team, 'logoFile') ~ '")' %}
                                        {% else %}
                                            {% set teamStyle = 'background: #123456;' %}
                                        {% endif %}
                                        <a href="{{ path('main_team_sso', {id: teamMember.team.id}) }}" class="team-img" style="{{ teamStyle }}"></a>
                                        <div class="team-info">
                                            <div class="team-name">
                                                {{ teamMember.team }}                                                    
                                            </div>
                                            <div class="team-details">
                                                <span>{{ 'message.roles'|trans }}:</span> {{ teamMember.roles|join(', ') }}
                                            </div>
                                            <div class="team-details">
                                                {% if teamMember.team.teamMembers.count > "1" %}
                                                    {{ teamMember.team.teamMembers.count }} {{ 'message.members'|trans }}
                                                {% else %}
                                                    {{ teamMember.team.teamMembers.count }} {{ 'message.member'|trans }}
                                                {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-auto text-right">
                                        <a href="{{ path('main_team_sso', {id: teamMember.team.id}) }}" class="button button-small">
                                            {{ 'button.login'|trans }}
                                        </a>
                                    </div>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                {% endif %}


                {% if app.user.teams.count == 0 and app.user.teamMembers.count == 0 %}
                <div class="col-xl-12" style="text-align: center;">
                    <p>Looks like you are new here, why don't you go ahead and <a href="">create a new team</a>?</p>
                </div>
                {% endif %}
                <div class="col-lg-1"></div>
            </div>
        </section>
    </div>
